.detail {
    display: flex;
    flex-direction: column;
    height: 100%;
}
.detail-swiper {
    height: 100%;
    
}
.detail-swiper-item {
    display: flex;
}
.detail-box {
    background-color: #ffffff;
    box-shadow: 0px 0px 10px #dddddd;
    margin: 20rpx 20rpx 40rpx 20rpx;
    border: 1rpx solid #dddddd;
    border-radius: 10rpx;
    padding: 0rpx 20rpx;
    position: relative;
    display: flex;
    flex-direction: column;
    width: 100%;
}
.detail-content-box {
    flex: 1;
    padding: 40rpx 0rpx;
}
.detail-title {
    font-size:40rpx;
}
.detail-content {
    margin-top: 40rpx;
}
.detail-foot {
    height: 100rpx;
    line-height: 100rpx;
    display: flex;
    justify-content: space-between;
    border-top: 1rpx solid #dddddd;
}
.detail-share {
    background: url('data:image/png;base64,');
    background-position:center;
    background-size:cover;
    width: 50rpx;
    height: 50rpx;
    position: relative;
    top: 25rpx;
}
.loading-component {
    height: 100%;
}
.padding-left10{
    padding-left: 10rpx;
}
